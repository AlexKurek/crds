# This .tpn defines constraints related to array objects vs. header keywords.
# Required relationships between subarray keywords are defined in a subarray .tpn
# This file pertains to array properties and their relationships to header keywords.

# SCI       A           I         F             IMAGE,==,(2048;2048),float64
# SCI       A           I         S             IMAGE,<=,(2048;2048),float64
# DQ       A           I         F             IMAGE,==,(2048;2048),float64
# ERR       A           I         S             IMAGE,<=,(2048;2048),float64
# DQ        A           I         F             IMAGE,>=,(2048;2048),uint8
# DQ        A           I         S             IMAGE,<=,(2048;2048),uint8
# DQ_DE     A           T         R             TABLE,([1-9]+[01-9]+;4),[J;J;40A;80A]

ERR       A           X         F             (META_SUBARRAY_XSTART==1)
ERR       A           X         F             (META_SUBARRAY_YSTART==1)
ERR       A           X         F             (ERR_ARRAY.SHAPE[0]==2048)
ERR       A           X         F             (ERR_ARRAY.SHAPE[1]==2048)

ERR       A           X         S             (ERR_ARRAY.SHAPE[0]<=2048)
ERR       A           X         S             (ERR_ARRAY.SHAPE[1]<=2048)
ERR       A           X         S             (ERR_ARRAY.SHAPE[0]==META_SUBARRAY_XSIZE)
ERR       A           X         S             (ERR_ARRAY.SHAPE[1]==META_SUBARRAY_YSIZE)


