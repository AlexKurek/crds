#! /bin/tcsh

cd `dirname $0`

./clean

foreach dir (lib hst client)
    if (-e $dir) then
    	echo "Installing $dir"
		(cd $dir;  python setup.py install --force)  > /dev/null
    else
		echo "Skipping $dir"
    endif
end

echo "Installing django-json-rpc"
tar zxf third_party/django-json-rpc-0.6.1.pygz
cd django-json-rpc*
python setup.py install $* --force >/dev/null
cd ..
rm -rf django-json-rpc*


./clean

